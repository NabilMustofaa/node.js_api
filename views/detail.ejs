<h1 class="text-center">List Article Related to <%= nama %></h1>

<ol class="list-group list-group-numbered mt-2">
    <% data.forEach(dat => { %>
        <% dat['affiliation'].forEach(el => { %>
          <% if (el['affilname'] == 'Universitas Airlangga') { %>
            <li class="list-group-item d-flex justify-content-between align-items-start">
              <div class="ms-2 me-auto">
                <div class="fw-bold"><%= dat['dc:title'] %></div>
                <%= dat['dc:creator'] %><br>
                <%= el['affilname'] %>
              </div>
            </li>
          <% } %> 
        <% }) %>
    <% }) %>

